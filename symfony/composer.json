{
    "name": "entropy/tunkki",
    "license": "MIT",
    "type": "project",
    "description": "Entropy Tunkki",
    "autoload": {
		"psr-4": { "App\\": "src/" }
    },
    "require": {
        "php": ">=7",
        "doctrine/doctrine-bundle": "^1.0",
        "friendsofsymfony/user-bundle": "^2",
        "hashids/hashids": "^3.0",
        "incenteev/composer-parameter-handler": "^2.1.2",
        "knpuniversity/oauth2-client-bundle": "^1.12",
        "sensio/framework-extra-bundle": "^5.0",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/classification-bundle": "^3",
        "sonata-project/core-bundle": "^3.0",
        "sonata-project/datagrid-bundle": "^2.2",
        "sonata-project/doctrine-orm-admin-bundle": "^3",
        "sonata-project/formatter-bundle": "^4.1",
        "sonata-project/intl-bundle": "^2.7",
        "sonata-project/media-bundle": "^3.0",
        "sonata-project/page-bundle": "^3.0",
        "sonata-project/seo-bundle": "^2.0",
        "sonata-project/timeline-bundle": "^3.0",
        "sonata-project/translation-bundle": "^2",
        "sonata-project/user-bundle": "^4",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "^4.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/monolog-bridge": "^4.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "twig/extensions": "^1.0"
    },
     "conflict": {
         "symfony/symfony": "*"
    },
    "require-dev": {
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1",
        "symfony/profiler-pack": "^1.0"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "bin/console cache:clear --ansi",
            "bin/console assets:install"
        ],
        "post-update-cmd": [
            "bin/console cache:clear --ansi",
            "bin/console assets:install"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "copy",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
