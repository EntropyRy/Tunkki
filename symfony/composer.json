{
    "name": "entropy/tunkki",
    "license": "MIT",
    "type": "project",
    "description": "Entropy Tunkki",
    "autoload": {
		"psr-4": { "App\\": "src/" }
    },
    "require": {
        "php": "^8.1",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/migrations": "3.5.2",
        "doctrine/orm": "^2",
        "gedmo/doctrine-extensions": "^3.9",
        "hashids/hashids": "^4.0",
        "knpuniversity/oauth2-client-bundle": "^2",
        "league/oauth2-server-bundle": "^0.4",
        "picqer/php-barcode-generator": "^2.2",
        "sonata-project/admin-bundle": "^4",
        "sonata-project/classification-bundle": "^4",
        "sonata-project/doctrine-orm-admin-bundle": "^4",
        "sonata-project/entity-audit-bundle": "^1.8",
        "sonata-project/formatter-bundle": "^5",
        "sonata-project/media-bundle": "^4.0",
        "sonata-project/page-bundle": "^4.0",
        "sonata-project/seo-bundle": "^3.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "^6.2",
        "symfony/dotenv": "^6.2",
        "symfony/flex": "^2.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.2",
        "symfony/mailer": "^6.2",
        "symfony/monolog-bridge": "^6.2",
        "symfony/monolog-bundle": "^3.7",
        "symfony/ux-turbo": "^2.4",
        "symfony/web-link": "6.2.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^6.2",
        "symfonycasts/reset-password-bundle": "^1.1",
        "twig/extra-bundle": "^3.4",
        "twig/intl-extra": "^3.4",
        "twig/string-extra": "^3.0"
    },
     "conflict": {
         "symfony/symfony": "*"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-symfony": "^1.1",
        "symfony/debug-bundle": "^6.2",
        "rector/rector": "^0.15",
        "symfony/maker-bundle": "^1",
        "symfony/stopwatch": "^6.2",
        "symfony/web-profiler-bundle": "^6.2"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "ckeditor:install --clear=drop --release=custom --custom-build-id=af60cce815fe22eaeca1943ebc8d2a25": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "phpstan/extension-installer": true
        }
    },
    "prefer-stable": true,
    "extra": {
        "symfony-assets-install": "copy",
        "symfony": {
            "allow-contrib": true
        }
    }
}
