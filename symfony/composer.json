{
    "name": "entropy/tunkki",
    "license": "MIT",
    "type": "project",
    "description": "Entropy Tunkki",
    "autoload": {
		"psr-4": { "App\\": "src/" }
    },
    "require": {
        "php": ">=7",
        "doctrine/doctrine-bundle": "^2.0",
        "friendsofsymfony/user-bundle": "^2",
        "hashids/hashids": "^3.0",
        "knpuniversity/oauth2-client-bundle": "^2",
        "sensio/framework-extra-bundle": "^5.0",
        "simplethings/entity-audit-bundle": "^1.0",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/classification-bundle": "^3",
        "sonata-project/doctrine-orm-admin-bundle": "^3",
        "sonata-project/formatter-bundle": "^4.1",
        "sonata-project/media-bundle": "^3.0",
        "sonata-project/page-bundle": "^3.14",
        "sonata-project/seo-bundle": "^2.0",
        "sonata-project/timeline-bundle": "^3.0",
        "sonata-project/translation-bundle": "^2",
        "sonata-project/user-bundle": "^4",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "^4.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/http-client": "^4.1",
        "symfony/monolog-bridge": "^4.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^4.1"
    },
     "conflict": {
         "symfony/symfony": "*"
    },
    "require-dev": {
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1",
        "symfony/profiler-pack": "^1.0"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "extra": {
        "symfony-assets-install": "copy"
    }
}
