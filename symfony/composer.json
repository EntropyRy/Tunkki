{
    "name": "entropy/tunkki",
    "license": "MIT",
    "type": "project",
    "description": "Entropy Tunkki",
    "autoload": {
		"psr-4": { "App\\": "src/" }
    },
    "require": {
        "php": ">=7",
        "beberlei/doctrineextensions": "^1.2",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2",
        "hashids/hashids": "^3.0",
        "knpuniversity/oauth2-client-bundle": "^2",
        "nyholm/psr7": "^1.3",
        "picqer/php-barcode-generator": "^2.1",
        "sensio/framework-extra-bundle": "^5.0",
        "sonata-project/admin-bundle": "^3",
        "sonata-project/classification-bundle": "^3",
        "sonata-project/doctrine-orm-admin-bundle": "^3",
        "sonata-project/entity-audit-bundle": "^1.0",
        "sonata-project/formatter-bundle": "^4.1",
        "sonata-project/media-bundle": "^3.0",
        "sonata-project/page-bundle": "^3.14",
        "sonata-project/seo-bundle": "^2.0",
        "sonata-project/translation-bundle": "^2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/http-client": "^4.1",
        "symfony/mailer": "^4.1",
        "symfony/monolog-bridge": "^4.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^4.1",
        "symfonycasts/reset-password-bundle": "^1.1",
        "trikoder/oauth2-bundle": "^3.1",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/string-extra": "^3.0"
    },
     "conflict": {
         "symfony/symfony": "*"
    },
    "require-dev": {
        "symfony/debug-bundle": "^5.1",
        "symfony/maker-bundle": "^1",
        "symfony/stopwatch": "^5.3",
        "symfony/web-profiler-bundle": "^5.0"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop --release=custom --custom-build-id=af60cce815fe22eaeca1943ebc8d2a25": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "extra": {
        "symfony-assets-install": "copy"
    }
}
